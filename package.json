  },
  "devDependencies": {
    "@jzfe/eslint-config-vue": "^1.6.1",
    "@rushstack/eslint-patch": "^1.2.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vitejs/plugin-vue-jsx": "^3.0.0",
    "eslint": "^8.30.0",
    "husky": "^8.0.2",
    "less": "^4.1.3",
    "lint-staged": "^13.1.0",
    "unplugin-vue-components": "^0.22.12",
